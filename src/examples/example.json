{
	"title": "A registration form",
	"description": "A simple form example.",
	"type": "object",
	"properties": {
		"firstName": {
			"type": "string",
			"minLength": 3,
			"label": "First name",
			"required": ["firstName"]
		},
		"phoneNumber": {
			"type": "string",
			"pattern": "[0-9]{11}",
			"required": ["firstName"],
			"options": {
				"placeholder": "+7(___)___-__-__"
			},
			"label": "Last name"
		},
		"Group": {
			"type": "object",
			"label": "GroupName",
			"properties": {
				"gender": {
					"type": "string",
					"label": "Gender",
					"enum": ["", "male", "female"]
				}
			}
		}
	}
}
